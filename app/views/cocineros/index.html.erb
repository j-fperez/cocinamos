<h1>Listando Cocineros</h1>

<table>
	<tr>
		<th>Nombre</th>
		<th>Apellido 1</th>
		<th>Apellido 2</th>
		<th>DNI</th>
		<th>Alias</th>
		<th colspan="5"></th>
	</tr>

	<% @cocineros.each do |cocinero| %>
		<tr>
			<td><%= cocinero.nombre_cocinero %></td>
			<td><%= cocinero.apellido_1_cocinero %></td>
			<td><%= cocinero.apellido_2_cocinero %></td>
			<td><%= cocinero.dni_cocinero %></td>
			<td><%= cocinero.alias_cocinero %></td>
			<td><%= link_to 'Ver', cocinero_path(cocinero) %></td>
      		<td><%= link_to 'Editar', edit_cocinero_path(cocinero) %></td>
      		<td><%= link_to 'Borrar', cocinero_path(cocinero), method: :delete, data: { confirm: '¿Estàs seguro?' } %></td>
		</tr>
	<% end %>
</table>

<%= link_to 'Volver', index_path %> |
